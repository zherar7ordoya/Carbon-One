                "fontStyle": ""
            }
        },
        //
        // Bold
        //
        {
            "scope": ["markup.bold"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        //
        // Italic
        //
        {
            "scope": [
                "entity.name.tag.css",
