                "punctuation.section.property-list",
                "string.other",
                "support.type.builtin",
                "support.type.primitive",
                "support.type.python"
            ],
            "settings": {
                "foreground": "#0f62fe"
            }
        },
        //
        // Green 60
        //
        {
            "scope": [
                "entity.name.type.module",
                "markup.inline",
                "meta.objectliteral",
                "punctuation.definition.string",
                "string.quoted",
                "string.template",
                "string.unquoted"
