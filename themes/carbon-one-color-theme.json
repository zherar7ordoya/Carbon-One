{
    //
    // THEME METADATA
    //
    "name": "Carbon One",
    "type": "light",
    "semanticHighlighting": false,
    "semanticTokenColors": {},
    //
    // ENVIRONMENT COLOR
    //
    "colors": {
        //
        // Visual Studio 2022
        //
        "activityBar.background": "#e8e8ec",
        "activityBar.border": "#bdc4d1",
        "activityBar.foreground": "#000000",
        "breadcrumb.background": "#5d6b99",
        "breadcrumb.foreground": "#ffffff",
        "dropdown.background": "#ffffff",
        "editorGroupHeader.tabsBackground": "#ccd5f0",
        "list.activeSelectionBackground": "#00a2ff36",
        "list.activeSelectionForeground": "#000000c2",
        "list.hoverBackground": "#00a2ff2a",
        "list.inactiveSelectionBackground": "#00a2ff1e",
        "notificationCenterHeader.background": "#40568d",
        "notificationCenterHeader.foreground": "#ffffff",
        "notifications.background": "#40568d",
        "notifications.foreground": "#ffffff",
        "panel.background": "#f7f9fe",
        "panel.border": "#5d6b99",
        "panelTitle.activeBorder": "#5d6b99",
        "panelTitle.activeForeground": "#5d6b99",
        "panelTitle.inactiveForeground": "#5d6b99",
        "scrollbarSlider.background": "#c2c3c9",
        "sideBar.background": "#f7f9fe",
        "sideBar.border": "#adb4bd",
        "sideBarSectionHeader.background": "#f7f9fe",
        "sideBarSectionHeader.border": "#adb4bd",
        "statusBar.background": "#40508d",
        "tab.activeBackground": "#f5cc84",
        "tab.activeBorder": "#f5cc84",
        "tab.border": "#ccd5f0",
        "tab.hoverBackground": "#ccd5f0",
        "tab.inactiveBackground": "#3b4f81",
        "tab.inactiveForeground": "#ffffff",
        "titleBar.activeBackground": "#ccd5f0",
        //
        // Campbell PowerShell
        //
        "terminal.ansiBlack": "#0C0C0C",
        "terminal.ansiBlue": "#0037DA",
        "terminal.ansiBrightBlack": "#767676",
        "terminal.ansiBrightBlue": "#3B78FF",
        "terminal.ansiBrightCyan": "#61D6D6",
        "terminal.ansiBrightGreen": "#16C60C",
        "terminal.ansiBrightMagenta": "#B4009E",
        "terminal.ansiBrightRed": "#E74856",
        "terminal.ansiBrightWhite": "#F2F2F2",
        "terminal.ansiBrightYellow": "#F9F1A5",
        "terminal.ansiCyan": "#3A96DD",
        "terminal.ansiGreen": "#13A10E",
        "terminal.ansiMagenta": "#881798",
        "terminal.ansiRed": "#C50F1F",
        "terminal.ansiWhite": "#CCCCCC",
        "terminal.ansiYellow": "#C19C00",
        "terminal.background": "#012456",
        "terminal.border": "#012456",
        "terminal.foreground": "#CCCCCC",
        "terminal.selectionBackground": "#FFFFFF",
        "terminalCursor.background": "#CCCCCC",
        "terminalCursor.foreground": "#FFFFFF",
        //
        // IBM Carbon
        //
        "editor.background": "#f4f4f4",
        "editor.foreground": "#161616"
    },
    //
    // SYNTAX HIGHLIGHTING
    //
    "tokenColors": [
        //
        // Red 60
        //
        {
            "scope": [
                "constant.other.database-name",
                "entity.name.section",
                "entity.name.tag.html",
                "entity.name.type.instance",
                "entity.name.type.tsx",
                "keyword.other.definition",
                "meta.decorator",
                "meta.function.variable",
                "meta.object-literal",
                "meta.property-list",
                "meta.type.parameters",
                "punctuation.definition.template-expression",
                "support.type.property-name",
                "support.variable",
                "variable.language",
                "variable.other.enummember",
                "variable.other.object",
                "variable.other.razor",
                "variable.other.readwrite.alias",
                "variable.other.regexp"
            ],
            "settings": {
                "foreground": "#da1e28"
            }
        },
        //
        // Blue 60
        //
        {
            "scope": [
                "constant.other.color",
                "entity.name.function.ts",
                "constant.language.null",
                "keyword.operator.decrement",
                "keyword.operator.expression.keyof",
                "keyword.operator.expression.typeof",
                "keyword.operator.increment",
                "keyword.operator.relational",
                "punctuation.definition.bracket",
                "punctuation.definition.group",
                "punctuation.definition.interpolation",
                "punctuation.section.embedded",
                "punctuation.section.property-list",
                "string.other",
                "support.type.builtin",
                "support.type.primitive",
                "support.type.python"
            ],
            "settings": {
                "foreground": "#0f62fe"
            }
        },
        //
        // Green 60
        //
        {
            "scope": [
                "entity.name.type.module",
                "markup.inline",
                "meta.objectliteral",
                "punctuation.definition.string",
                "string.quoted",
                "string.template",
                "string.unquoted"
            ],
            "settings": {
                "foreground": "#198038"
            }
        },
        //
        // Yellow Hover 70
        //
        {
            "scope": [
                "constant.numeric",
                "constant.other.caps",
                "constant.other.character-class",
                "entity.name.type.alias",
                "entity.name.type.class",
                "entity.name.type.config",
                "entity.name.type.interface",
                "entity.name.type.model",
                "entity.name.type.namespace",
                "entity.name.type.ts",
                "entity.other.attribute-name",
                "entity.other.inherited-class",
                "keyword.other.unit",
                "markup.bold",
                "meta.class",
                "meta.return.type",
                "punctuation.definition.bold",
                "support.class.promise",
                "variable.other.readwrite.ts"
            ],
            "settings": {
                "foreground": "#806000"
            }
        },
        //
        // Purple Hover 70
        //
        {
            "scope": [
                "entity.name.function.attribute",
                "keyword.control",
                "keyword.generator",
                "keyword.operator.arrow",
                "keyword.operator.expression.in",
                "keyword.operator.expression.is",
                "keyword.operator.expression.of",
                "keyword.operator.new",
                "keyword.other",
                "keyword.other.directive",
                "keyword.other.DDL",
                "keyword.other.DML",
                "keyword.preprocessor",
                "keyword.type",
                "markup.italic",
                "markup.underline",
                "punctuation.definition.italic",
                "storage"
            ],
            "settings": {
                "foreground": "#7c3dd6"
            }
        },
        //
        // Gray 100
        //
        {
            "scope": [
                "constant.language.import-export-all",
                "constant.other.table-name",
                "entity.name.variable",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "keyword.operator.quantifier",
                "keyword.operator.rest",
                "keyword.operator.spread",
                "keyword.operator.ternary",
                "keyword.operator.type",
                "meta.attribute",
                "meta.brace",
                "meta.function-call",
                "meta.function.parameters",
                "meta.paragraph",
                "meta.var-single-variable",
                "punctuation.accessor",
                "punctuation.curlybrace",
                "punctuation.decorator",
                "punctuation.definition.array",
                "punctuation.definition.block",
                "punctuation.definition.constant",
                "punctuation.definition.dictionary",
                "punctuation.definition.entity",
                "punctuation.definition.inheritance",
                "punctuation.definition.ng-binding-name",
                "punctuation.definition.parameters",
                "punctuation.definition.tag",
                "punctuation.definition.typeparameters",
                "punctuation.parenthesis",
                "punctuation.separator",
                "punctuation.squarebracket",
                "punctuation.terminator",
                "text",
                "variable.object",
                "variable.other.constant",
                "variable.other.jsdoc",
                "variable.other.object.property",
                "variable.other.property",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        //
        // Gray 60
        //
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#6f6f6f"
            }
        },
        //
        // Cyan 60
        //
        {
            "scope": [
                "support.function",
                "constant.language.boolean",
                "support.constant"
            ],
            "settings": {
                "foreground": "#0072c3"
            }
        },
        // Reset Style
        //
        {
            "scope": [
                "entity.name.type.ts",
                "entity.name.type.tsx",
                "keyword.operator",
                "punctuation.definition.ng-binding-name",
                "punctuation.separator",
                "storage.type.function",
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        //
        // Bold
        //
        {
            "scope": ["markup.bold"],
            "settings": {
                "fontStyle": "bold"
            }
        },
        //
        // Italic
        //
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.variable",
                "markup.quote",
                "meta.var-single-variable",
                "variable.object.property",
                "variable.other.assignment",
                "variable.other.constant",
                "variable.other.jsdoc",
                "variable.other.property",
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        }
    ]
}
